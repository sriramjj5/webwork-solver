{
    "manifest_version": 3,
    "name": "WebWork Solver",
    "version": "1.0",
    "description": "A Chrome extension to solve WebWork math assignments.",
    "permissions": ["activeTab", "scripting"],
    "background": {
      "service_worker": "background.js"
    },
    "action": {
      "default_popup": "popup.html"
    },
    "icons": {
      "48": "icon.png",
      "128": "icon.png"
    }
  }
  